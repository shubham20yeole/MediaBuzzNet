<div class="container detailView">
  <div class="row">
    <div class="col-sm-1">
    <div class="row"><h3>GROUP</h3></div>
      <div class="scrollableNotification">     
        <% documentscategory.forEach(function(documentscategory){%>
          <div class="limitchar cat-p-tag row" id="<%= documentscategory.catname %>">(<%= documentscategory.size %>) <span title="<%= documentscategory.catname %>" class="groupname"><%= documentscategory.catname %></span></div><br><br>
        <%})%>
      </div>
    </div>
    <div class="col-sm-1">
    <div class="row"><h3>ITEMS</h3></div>
      <div id="load"></div>
    <div class="scrollableNotification">     
     <div id="items">
         <% allDocuments.forEach(function(allDocuments){%>
          <div class="row">
              <p class="itemname limitchar"><span title="<%= allDocuments.name %>"><%= allDocuments.name %></span></p>
              <input type="hidden" value="<%= allDocuments.frame %>">
              <input type="hidden" value="<%= allDocuments.datetime %>">
              <input type="hidden" value="<%= allDocuments.category %>">
              <input type="hidden" value="<%= allDocuments.name %>">
              <input type="hidden" value="<%= allDocuments.link %>">
            </div>
        <%})%>
      </div>
    </div>
    </div>
    <div class="col-sm-8">
    <div class="container">
  <!-- Trigger the modal with a button -->

</div>
      <ul class="pager">
        <li><span>TIMELINE</span></li>
        <li><a href="0">First</a></li>
        <li><a href="<%=prepageno%>" class="prepageno">Previous</a></li>
        <li><a href="<%=nextpageno%>" class="nextpageno">Next</a></li>
        <li><a href="<%=lastpageno%>" class="lastpageno">Last</a></li>
        <li><span class="paginationmsg"></span></li>
        <li><input type="hidden" class="prehide" value="<%=prepageno%>"></li>
        <li><input type="hidden" class="nexthide" value="<%=nextpageno%>"></li>
        <li><input type="hidden" class="lasthide" value="<%=lastpageno%>"></li>
      </ul>
      <div id="load1"></div>        
      <div id="iframediv"></div>
      <p id="infodiv"></p>
      <div class="scrollableTimeLineView">
      <% if(prepageno==-1){%>

        <div class="mediasec">
        <div id="iframediv">

          <video controls style="background:url(images/audio4gif.gif) center center no-repeat; background-size: 100%; width: 100%; height: 100%" autoplay="true"><source src="http://songspkdownloads.in/English%20Single/The%20Greatest%20-%20320Kbps%20-%20(www.songspksongspk.cc).mp3" type='video/mp4'></video>
        </div>
          <div>
            <p>
              <span><b>Category:</b> </span>
              <span style="float: right;"><b>Date&Time:</b> </span>
            </p>
            <p><b>Media Name:</b> <%= documents.name %></p>
            <a href="#" target="_blank"><b>View in next Tab?</b></a>
          </div>
        </div>
        <hr>
    <%}%>
      <% documents.forEach(function(documents){%>
      <div class="mediasec">
        <div id="iframediv"><%- documents.frame %></div>
          <input type="hidden" value="<%= documents.frame %>">
          <input type="hidden" value="<%= documents.datetime %>">
          <input type="hidden" value="<%= documents.category %>">
          <div>
            <p>
              <span><b>Category:</b> <%= documents.category %></span>
              <span style="float: right;"><b>Date&Time:</b> <%= documents.datetime %></span>
            </p>
            <p><b>Media Name:</b> <%= documents.name %></p>
            <a href="<%= documents.link %>" target="_blank"><b>View in next Tab?</b></a>
          </div>
        </div>
        <hr>
        <%})%>
        </div>
        <ul class="pager">
        <li><span>TIMELINE</span></li>
        <li><a href="0">First</a></li>
        <li><a href="<%=prepageno%>" class="prepageno">Previous</a></li>
        <li><a href="<%=nextpageno%>" class="nextpageno">Next</a></li>
        <li><a href="<%=lastpageno%>" class="lastpageno">Last</a></li>
        <li><span class="paginationmsg"></span></li>
        <li><input type="hidden" class="prehide" value="<%=prepageno%>"></li>
        <li><input type="hidden" class="nexthide" value="<%=nextpageno%>"></li>
        <li><input type="hidden" class="lasthide" value="<%=lastpageno%>"></li>
      </ul>
    </div>
     <div class="col-sm-2">
      <h3>NOTIFICATION</h3>
      <div class="scrollableNotification">     
        <% allDocuments.forEach(function(allDocuments){%>
          <div class="row">
            <div class="col-sm-5">
              <img class="img-thumbnail" src="<%= allDocuments.thumbnail %>" width="100" height="100">
            </div>
            <div class="col-sm-7">
              <span class="notificationDate"><%= allDocuments.datetime %></span><br>
              <p class="itemname limitchar">
                <span title="<%= allDocuments.name %>">
                  <%= allDocuments.name %>
                </span>
              </p>
              <input type="hidden" value="<%= allDocuments.frame %>">
              <input type="hidden" value="<%= allDocuments.datetime %>">
              <input type="hidden" value="<%= allDocuments.category %>">
              <input type="hidden" value="<%= allDocuments.name %>">
              <input type="hidden" value="<%= allDocuments.link %>">
            </div>
          </div>
        <%})%>
      </div>
    </div>
  </div>
</div>